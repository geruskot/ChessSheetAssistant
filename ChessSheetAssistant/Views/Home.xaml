<Page x:Class="ChessSheetAssistant.Views.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChessSheetAssistant.Views"
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Home">

    <Grid>
        <Frame x:Name="HomeFrame" NavigationUIVisibility="Hidden" ></Frame>
        <Button
            HorizontalAlignment="Left"
            Background="{x:Null}"
            BorderBrush="{x:Null}"
            Click="Button_Click"
            >
            <materialDesign:PackIcon Kind="NavigateBefore"></materialDesign:PackIcon>
        </Button>
    </Grid>
</Page>
